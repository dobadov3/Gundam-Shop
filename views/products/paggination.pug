nav(aria-label='Page navigation example')
    ul.pagination.justify-content-center
        li.page-item.disabled
            a.page-link(href='#', tabindex='-1', aria-disabled='true') 
                i.fal.fa-chevron-left
        li.page-item
            a.page-link(href='#') 1
        li.page-item
            a.page-link(href='#') 2
        li.page-item
            a.page-link(href='#') 3
        li.page-item
            a.page-link(href='#') 
                i.fal.fa-chevron-right